<div class="{{CG_input block='input-wrapper'}}">
    <input id={{id}} name="{{name}}" type={{type}} class="{{CG_input modifiers=modifiers}}" placeholder=' ' onfocus='this.removeAttribute("readonly")' readonly }>
    <label for="{{id}}" class="{{CG_input block='input-label'}}">{{placeholder}}</label>
</div>



